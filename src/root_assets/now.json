{
  "name": "LLCT-CDN",
  "version": 2,
  "alias": ["lovelivec.kr", "www.lovelivec.kr"],
  "regions": ["icn1", "hnd1"],
  "routes": [
    {
      "src": "/data/(.*)",
      "headers": { "cache-control": "s-maxage=86400" },
      "dest": "/data/$1"
    }
  ],
  "builds": [{ "src": "**", "use": "@now/static" }]
}
