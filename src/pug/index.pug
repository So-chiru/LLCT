    style.
        .hide_curtain {opacity:0;visibility: none;pointer-events: none;}
  body
    div#curt(style="display:flex;position:fixed;width:100%;height:100%;z-index:100;transition: 0.3s cubic-bezier(0.19, 1, 0.22, 1);")
        .inr
            h1 LLCT
            svg.line_svg(data-type="Line")
                path.line_svg_class#__3(d="M 0 0 L 323 0")
            h3 LoveLive! Call Table Guide
    .contents_collection
        .full_page_ct
            .header
                .titles
                    span <span id="genki_year">2017</span>년 <span id="zenkai_month">11</span>월 <span id="day_day">18</span>일 <span id="day">18</span>요일
                    h1 노래 목록
                .tabs
                    span.active 전체
                    span(onclick="alert('이미 선택된 좌석입니다.');") 라이브
                    span(onclick="alert('이미 선택된 좌석입니다.');") 추천
            .lds-ellipsis.loader#loading_spin_ctlst
                div
                div
                div
                div
            .call_lists#call_lists
            .navigator
                span(onclick="pageAdjust.prevPage()") <i class="material-icons arrow_btn">keyboard_arrow_left</i>
                span.pg_ind <span id="currentPage">1</span> <span>/</span> <span id="totalPage">6</span>
                span(onclick="pageAdjust.nextPage()") <i class="material-icons arrow_btn">keyboard_arrow_right</i>
    .player_bg(onclick="yohaneNoDOM.chiisakuni()")
    #kara_player.player.hide
        .visible_dash
            .info
                img#album_meta.c.cover
                .info_txts
                    p#title_meta.c.title 브로콜리
                    .c.diver
                    p#artist_meta.c.artist 세이라
                .c.diver.after_div
            .timeline
                p#played_time.c.mga 0 : 00
                input#ctrl_time.hareru.c(type="range", min="0" max="100" step="0.1" value="0" name="" oninput="yohane.seekTo( this.value )")
                p#left_time.c.mga - 4 : 59
            .ctrls
                span#pp_btn.material-icons(onclick="yohane.toggle()") pause
                span#live_btn.material-icons.in_active(onclick="yohane.liveEffects()") room_service
                span#dekaku_btn.material-icons(onclick="yohaneNoDOM.kaizu ? yohaneNoDOM.chiisakuni() : yohaneNoDOM.dekakuni()") keyboard_arrow_down
                
        .lyrics_wrapper
            p
              | 블레이드 색상 :
              span#blade_color 자유
            .karaoke#karaoke

        audio#kara_audio
    #PLEASE_STOP_USE_INTERNET_EXPLORER
