        .hide_curtain {opacity:0;visibility: none;pointer-events: none;z-index: -1;}
  body
    div#curt(style="display:flex;position:fixed;width:100%;height:100%;z-index:100;transition: 1s cubic-bezier(0.19, 1, 0.22, 1);background-color:#16aaf9;")
        .inr(style="margin: auto;position: relative;text-align: center;")
            h1 LLCT
            h3 LoveLive! Call Table Guide
    .player_bg(onclick="yohaneNoDOM.chiisakuni()")
    #kara_player.player.hide(onclick="yohaneNoDOM.dekakuOnce(this.target)")
        .dash_wrapper#dash_wrp_ham(data-scrollbar)
            .visible_dash
                .info
                    img#album_meta.c.cover
                    .info_txts
                        p#title_meta.c.title 브로콜리
                        .c.diver.spaRight
                        p#artist_meta.c.artist 세이라
                    .c.diver.after_div
                .timeline
                    p#played_time.c.mga 0 : 00
                    #ctrl_times.c.__progress
                        .passed_bar#psd_times
                        .max_bar.__buf#bar_eventListen
                        .thumb#__current_thumb
                    p#left_time.c.mga - 4 : 59
                .ctrls
                    span#pp_btn.material-icons(onclick="yohane.toggle()" title="노래 재생") play_arrow
                    // span#live_btn.material-icons.in_active(onclick="yohane.liveEffects()") room_service
                    span#tick_btn.material-icons.in_active(onclick="Karaoke.toggleTickSounds()" title="콜에 틱 소리 내기") alarm
                    span#pl_loop.material-icons.in_active(onclick="yohane.toggleLoops()" title="(현재 노래 / 플레이 리스트) 반복 / 순차 재생") loop
                    span#dekaku_btn.material-icons(title="플레이어 크게 / 작게") keyboard_arrow_down
        .lyrics_wrapper
            p.blade_c
                | 블레이드 추천 색상 : 
                span#blade_color 자유
            p.is_singalong#sing_tg 이 곡은 떼창 곡 입니다. 지시에 맞게 따라 불러주세요.
            .karaoke#karaoke
            p.bottom_margin
        audio#kara_audio
        audio#tick_sounds(src="/live_assets/key-press-2.mp3")
    .contents_collection
        .full_page_ct#fp_ct
            .header
                .titles
                    span <span id="genki_year">2017</span>년 <span id="zenkai_month">11</span>월 <span id="day_day">18</span>일 <span id="day">18</span>요일
                    h1#p_title 노래 목록
                .tabs
                    // span.active 전체
                    //- span#pl_ms_switcher.ti.min_ti(onclick="LLCT.currentPage === 0 ? LLCT.viewPlayLists() : LLCT.viewMusicLists()") <span class="material-icons ti">queue_music</span>
                    span.ti.min_ti(onclick="yohane.shuffle()") <span class="material-icons ti">shuffle</span>
                    span.ti.min_ti(onclick="LLCT.showSetting()") <span class="material-icons ti">settings</span>
                    //- .search_wrp.ti(onclick="$('#search_val').focus()")
                        input#search_val(type="text", name="")
                        span(onclick="yohaneNoDOM.search()") <span class="material-icons ti">search</span>
            .lds-ellipsis.loader#loading_spin_ctlst
                div
                div
                div
                div
            ._card_lists#_card_lists
            .navigator
                span(onclick="pageAdjust.prevPage()") <i class="material-icons arrow_btn thin">keyboard_arrow_left</i>
                span.pg_ind <span class="thin" id="currentPage" class="thin">1</span> <span>/</span> <span class="black" id="totalPage">7</span>
                span(onclick="pageAdjust.nextPage()") <i class="material-icons arrow_btn thin">keyboard_arrow_right</i>
    .setting_layer.hide
        .close_btn(onclick="LLCT.hideSetting()")
            p <i class="material-icons">close</i>
        .setting_sc
            h1 설정
            br
            .toggle_collection
                label.ckbox 번역된 제목으로 보기 <input id="showAs_translated" class="toggle_input" type="checkbox" checked="none" onclick="changedOption(0, this.checked)"> <label for="showAs_translated" class="toggle_btn"><span></span></label>   
            .toggle_collection
                label.ckbox 다크 모드 <input id="darkMode" class="toggle_input" type="checkbox" checked="none" onclick="changedOption(1, this.checked)"><label for="darkMode" class="toggle_btn"><span></span></label>
            .toggle_collection
                label.ckbox 사용 가능한 경우 인터렉티브 콜표 사용 (미사용시 콜표 이미지를 대신 표시) <input id="useInteractiveCall" class="toggle_input" type="checkbox" checked="none" onclick="changedOption(2, this.checked)"><label for="useInteractiveCall" class="toggle_btn"><span></span></label>
            .toggle_collection
                label.ckbox 음악 플레이어 끄기 (활성시 콜표 이미지만 표시) <input id="doNotUseMusicPlayer" class="toggle_input" type="checkbox" checked="none" onclick="changedOption(3, this.checked)"><label for="doNotUseMusicPlayer" class="toggle_btn"><span></span></label>
            .toggle_collection
                label.ckbox 앨범 이미지 사용 <input id="useAlbumImage" class="toggle_input" type="checkbox" checked="none" onclick="changedOption(4, this.checked)"><label for="useAlbumImage" class="toggle_btn"><span></span></label>
            .toggle_collection
                label.ckbox 고급 : requestAnimationFrame 대신 setInterval 사용하기 <input id="useSetIntervalInstead" class="toggle_input" type="checkbox" checked="none" onclick="changedOption(5, this.checked)"><label for="useSetIntervalInstead" class="toggle_btn"><span></span></label>

